<Page
	x:Class="ClubApplication.Views.MenuPage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:ClubApplication.Views"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Title="MenuPage"
	HorizontalAlignment="Center"
	VerticalAlignment="Center"
	d:DesignHeight="210"
	d:DesignWidth="200"
	Loaded="MenuPage_OnLoaded"
	mc:Ignorable="d">

	<StackPanel Width="200" Orientation="Vertical">
		<Button
			Name="BtnClubsMove"
			Click="BtnClubsMove_OnClick"
			Content="Кружки" />
		<Button
			Name="BtnChildrenMove"
			Click="BtnChildrenMove_OnClick"
			Content="Дети" />
		<Button
			Name="BtnParentsMove"
			Click="BtnParentsMove_OnClick"
			Content="Родители" />
		<Button
			Name="BtnUsersMove"
			Click="BtnUsersMove_OnClick"
			Content="Пользователи" />
	</StackPanel>
</Page>
